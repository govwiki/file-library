<div id="{{- id -}}" class="modal-wrapper">
  <div class="modal">
    <div class="modal-header">
      <span class="modal-close" data-modal-close>&times;</span>
      <h3 class="modal-title">{{- title -}}</h3>
    </div>
    <div class="modal-content">
        {%- block content -%}
        {%- endblock content -%}
    </div>
  </div>
</div>