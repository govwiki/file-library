{% extends 'layout.twig' %}

{% block main %}
    <h1>CAFRS and Single Audits for US Governments and Non-Profits</h1>

    <table class="sortable">
        <thead>
            <tr>
                <th>Type</th>
            </tr>
        </thead>
        <tbody>
            {%- for type in types -%}
                <tr class="dir">
                    <td><a href="{{ path_for('states', { type: type }) }}">{{- type -}}</a></td>
                </tr>
            {%- endfor -%}
        </tbody>
    </table>
{% endblock main %}